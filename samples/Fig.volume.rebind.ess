loadModel('Fig.volume.rebind.eml')
validationProcessStub = createEntityStub('Process:/:validate')
duration = validationProcessStub.getProperty('LogDuration')
interval = validationProcessStub.getProperty('LogInterval')
if(interval != -1):
  duration = duration+interval+interval*0.0000001;
else:
  duration = duration+duration*0.1;
message("starting simulation...")
run(duration)
message("done.")
